{
  "Tenants": 
  [

    {
      "Authority": "{secret}",
      "Audience": "{secret}",
      "IsDefaultScheme": "{secret}",
      "TenantName": "{secret}"
    }
  ],
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",

  "ConnectionStrings": {
    "Database": "{secret}"
  },


    "Serilog": {
      "Using": [
        "Serilog.Sinks.ApplicationInsights"
      ],
      "WriteTo": [
        {
          "Name": "File",
          "Args": {
            "path": "log/api.json",
            "rollingInterval": "Day",
            "formatter": "Serilog.Formatting.Json.JsonFormatter, Serilog"
          }
        },
        {
          "Name": "File",
          "Args": {
            "path": "log/api.log",
            "rollingInterval": "Day"
          }
        },
        "Debug"
      ]
    }
  
}

